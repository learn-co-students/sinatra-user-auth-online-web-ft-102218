<h1>Log In Below:</h1>
<%  if @message %>
        <%= @message %>
<%  end %>
<form method="POST" action="/sessions">
    <p>Email: <input type="text" id="email" name="email"></p>
    <p>Password: <input type="text" id="password" name="password"></p>
    <p><input type="submit" value="Log In"></p>
</form>
